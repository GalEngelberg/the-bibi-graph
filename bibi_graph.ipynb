   "source": [
    "# Network diameter\n",
    "query = \"\"\"MATCH (n)\n",
    "            WITH collect(n) AS nodes\n",
    "            UNWIND nodes AS a\n",
    "            UNWIND nodes AS b\n",
    "            WITH a, b\n",
    "            WHERE id(a) < id(b)\n",
    "            MATCH path=shortestPath((a)-[*]-(b))\n",
    "            RETURN length(path) AS diameter\n",
    "            ORDER BY diameter\n",
    "            DESC LIMIT 1\"\"\"\n",
    "print(graph.run(query))"
   ]
